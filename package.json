    "@prisma/client": "^6.3.1",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "helmet": "^8.0.0",
    "ioredis": "^5.5.0",
    "prisma": "^6.3.1",
    "redis": "^4.7.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/ioredis": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
